# Table: `wp_adintegration`

# Delete any existing table `wp_adintegration`

DROP TABLE IF EXISTS `wp_adintegration`;

# Table structure of table `wp_adintegration`

CREATE TABLE `wp_adintegration` (
  `id` bigint(20) NOT NULL auto_increment,
  `user_login` varchar(60) default NULL,
  `failed_login_time` bigint(11) default NULL,
  UNIQUE KEY `id` (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=6397 DEFAULT CHARSET=latin1 ;

