# Table: `wp_bup_options_categories`

# Delete any existing table `wp_bup_options_categories`

DROP TABLE IF EXISTS `wp_bup_options_categories`;

# Table structure of table `wp_bup_options_categories`

CREATE TABLE `wp_bup_options_categories` (
  `id` int(11) NOT NULL auto_increment,
  `label` varchar(128) NOT NULL,
  PRIMARY KEY  (`id`),
  KEY `id` (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=5 DEFAULT CHARSET=utf8 ;

