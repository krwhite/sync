# Table: `wp_bp_user_blogs_blogmeta`

# Delete any existing table `wp_bp_user_blogs_blogmeta`

DROP TABLE IF EXISTS `wp_bp_user_blogs_blogmeta`;

# Table structure of table `wp_bp_user_blogs_blogmeta`

CREATE TABLE `wp_bp_user_blogs_blogmeta` (
  `id` bigint(20) NOT NULL auto_increment,
  `blog_id` bigint(20) NOT NULL,
  `meta_key` varchar(255) default NULL,
  `meta_value` longtext,
  PRIMARY KEY  (`id`),
  KEY `blog_id` (`blog_id`),
  KEY `meta_key` (`meta_key`)
) ENGINE=MyISAM AUTO_INCREMENT=4 DEFAULT CHARSET=utf8 ;

